<h1>Upload</h1>
<table class="table">
  <thead>
    <tr>
      <th>Upload</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= upload.id %></td>
        <td><%= upload.csv_file.filename %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<div class="mt-3 pagination pull-right">
  <%= paginate @uploads %>
</div>

<section class="py-3">
  <%= link_to t('New Upload'), new_upload_path %>
</section>

  